{
  "name": "klaus-bookmarks",
  "version": "0.1.0",
  "description": "A repo for my config",
  "homepage": "https://github.com/KlausDevWalker/klaus-bookmarks",
  "repository": "https://github.com/KlausDevWalker/klaus-bookmarks",
  "bugs": "https://github.com/KlausDevWalker/klaus-bookmarks/issues",
  "author": {
    "name": "Klaus Ferreira",
    "email": "klaus@codewalkers.org",
    "url": "https://klaus.codewalkers.org"
  },
  "scripts": {
    "commit": "commit",
    "release": "standard-version --sign",
    "release:minor": "standard-version --sign --release-as minor",
    "release:patch": "standard-version --sign --release-as patch",
    "release:major": "standard-version --sign --release-as major",
    "publish": "git push --follow-tags origin master"
  },
  "main": "index.js",
  "license": "GPL-3.0",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E $HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@commitlint/travis-cli": "latest",
    "husky": "latest",
    "standard-version": "latest"
  }
}
